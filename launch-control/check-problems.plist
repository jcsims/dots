<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Disabled</key>
	<false/>
	<key>Label</key>
	<string>check-problems</string>
	<key>Program</key>
	<string>/usr/local/bin/check-problems</string>
	<key>StandardErrorPath</key>
	<string>/tmp/check-problems.stderr</string>
	<key>StandardOutPath</key>
	<string>/tmp/check-problems.stdout</string>
	<key>StartCalendarInterval</key>
	<dict>
		<key>Minute</key>
		<integer>35</integer>
	</dict>
	<key>WatchPaths</key>
	<array>
		<string>~/problems</string>
	</array>
</dict>
</plist>
